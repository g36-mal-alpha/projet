	<queryString language="SQL">
		<![CDATA[SELECT benevole.nom,
	benevole.prenom,
	benevole.date_naissance,
	benevole.idbenevole,
	avoir.idposte,
	benevole.permanent,
	poste.lieu,
	poste.signaleur,
	poste.heure_debut,
	poste.numero_poste,
	poste.heure_fin
FROM avoir
	INNER JOIN benevole ON 
	 avoir.idbenevole = benevole.idbenevole 
	INNER JOIN poste ON 
	 avoir.idposte = poste.idposte]]>
	</queryString>
